{
  "name": "ai-tool",
  "displayName": "AI Tool",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "AI Tool",
      "properties": {
        "ai-tool.baseURL": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "https://api.openai.com/v1",
                "http://localhost:1234/v1"
              ]
            },
            {
              "type": "string"
            }
          ],
          "default": "https://api.openai.com/v1"
        },
        "ai-tool.apiKey": {
          "type": "string",
          "default": "sk-xxx"
        },
        "ai-tool.model": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "gpt-4-32k",
                "gpt-3.5-turbo-16k",
                "vicuna-13B-v1.5-16K"
              ]
            },
            {
              "type": "string"
            }
          ],
          "default": "gpt-4-32k"
        }
      }
    },
    "commands": [
      {
        "command": "ai-tool.generateCommitMessage",
        "title": "AI Tool: Generate Commit Message",
        "icon": {
          "dark": "resources/icons/24x24.png",
          "light": "resources/icons/24x24.png"
        }
      }
    ],
    "menus": {
      "scm/title": [
        {
          "command": "ai-tool.generateCommitMessage",
          "when": "scmProvider == git",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "node-fetch-commonjs": "^3.3.2"
  }
}
